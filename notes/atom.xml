---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Chunyang's Blog</title>
	<link href="https://wangchunyang.github.io/notes/atom.xml" rel="self"/>
	<link href="https://wangchunyang.github.io/notes"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://wangchunyang.github.io/notes</id>
	<author>
		<name>Chunyang Wang</name>
		<email>wangchunyang@gmail.com</email>
	</author>

	{% for note in site.notes %}
		<entry>
			<title>{{ note.title }}</title>
			<link href="https://wangchunyang.github.io{{ post.url }}"/>
			<updated>{{ note.date | date_to_xmlschema }}</updated>
			<id>https://wangchunyang.github.io{{note.id}}</id>
			<content type="html">{{note.content|xml_escape}}</content>
		</entry>
	{% endfor %}

</feed>
